{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "moduleDetection": "force",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": false,
    "resolveJsonModule": true,
    "verbatimModuleSyntax": true,
    "allowJs": false,
    "jsx": "react-jsx",
    "types": []
  }
}
