{
  "name": "@alphagate/main-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "biome check .",
    "test": "vitest run",
    "db:migrate": "bun run src/db/migrate.ts",
    "db:seed": "bun run src/db/seed.ts",
    "types:gen:local": "npx --yes supabase gen types typescript --local > src/types/supabase.ts",
    "types:gen:remote": "npx --yes supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.ts"
  },
  "dependencies": {
    "@alphagate/contracts": "workspace:*",
    "@aws-sdk/client-kms": "^3.883.0",
    "@aws-sdk/credential-providers": "^3.883.0",
    "@clickhouse/client": "^1.12.1",
    "@supabase/supabase-js": "^2.57.2",
    "dotenv": "^17.2.2",
    "hono": "^4.5.0",
    "kysely": "^0.28.5",
    "kysely-postgres-js": "^3.0.0",
    "postgres": "^3.4.7",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/bun": "^1.2.21",
    "@types/node": "^22.5.2",
    "kysely-supabase": "^0.2.1",
    "typescript": "^5.5.4"
  }
}
